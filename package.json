{
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "scripts": {
    "build:css": "tailwindcss -i ./workzone/static/css/styles.css -o ./workzone/static/css/output.css",
    "watch:css": "tailwindcss -i ./workzone/static/css/styles.css -o ./workzone/static/css/output.css --watch",
    "dev": "npm run watch:css",
    "start": "npm run build:css"
  }
}